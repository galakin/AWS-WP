{
  "family": "git-wp",
  "executionRoleArn": "arn:aws:iam::112233445566:role/ecsTaskExecutionRole",
  "containerDefinitions": [    
      {
        "cpu": 0,
        "image": "370599928061.dkr.ecr.us-east-2.amazonaws.com/gitaction-test:sql-latest",
        "mountPoints": [
            {
            "containerPath": "/var/lib/mysql",
            "sourceVolume": "wpvol"
            }
        ],
        "memory": 400,
        "essential": true,
        "name": "db"
      },
      {
          "name": "wp",
          "image": "370599928061.dkr.ecr.us-east-2.amazonaws.com/gitaction-test:wp-lates",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 80,
              "protocol": "tcp",
              "containerPort": 80
            }
          ],
          "memory": 400,
          "links": [
              "db"
          ],
          "essential": true
      }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "volumes": [
    {
      "name": "wpvol",
      "host": {
        "sourcePath": "/var/dbfiles"
      }
    }
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
